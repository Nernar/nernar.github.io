{
	"name": "Core Engine",
	"includeVersion": true,
	"readme": "none",
	"searchInComments": true,
	"intentionallyNotExported": [
		"native.Array"
	],
	"kindSortOrder": [
		"Document",
		"Reference",
		"Module",
		"Namespace",
		"Enum",
		"EnumMember",
		"Class",
		"Interface",
		"Constructor",
		"Property",
		"Variable",
		"Function",
		"Accessor",
		"Method",
		"TypeAlias"
	],
	"groupOrder": [
		"Document",
		"Reference",
		"Module",
		"Namespace",
		"Enum",
		"EnumMember",
		"Class",
		"Interface",
		"Constructor",
		"Property",
		"Variable",
		"Function",
		"Accessor",
		"Method",
		"TypeAlias"
	],
	// TODO: typedoc#lib\converter\plugins\InheritDocPlugin.js#207 crashes on UI module
	"entryPointStrategy": "expand",
	"visibilityFilters": {
		"inherited": true,
		"protected": true,
		"@internal": false,
		"@deprecated": true
	},
	"customCss": "media/custom.css",
	"navigationLinks": {
		"Docs": "https://nernar.github.io/docs/intro"
	},
	"theme": "typedoc-codicon",
	"plugin": [
		"typedoc-codicon-theme",
		"typedoc-plugin-merge-modules"
		// TODO: Settings like in @mxssfd/typedoc-theme.
		// TODO: Internals like in @gobstones/typedoc-theme-gobstones.
		// TODO: Page groups like in typedoc-github-theme.
	],
	"mergeModulesMergeMode": "project"
}
