{
  "name": "docusaurus-typedoc-plugin",
  "version": "4.4.0",
  "description": "Docusaurus plugin that provides source code API documentation powered by TypeDoc.",
  "keywords": [
    "docusaurus",
    "plugin",
    "typedoc",
    "api"
  ],
  "author": "Miles Johnson, Nernar",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "private": true,
  "files": [
    "assets/**/*",
    "lib/**/*",
    "src/**/*",
    "styles.css"
  ],
  "engines": {
    "node": ">=16.12.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "scripts": {
    "build": "tsc --build && node tools/copyUntypedFiles.js",
    "watch": "run-p -c copy:watch build:watch",
    "build:watch": "tsc --build --watch",
    "copy:watch": "node tools/copyUntypedFiles.js --watch"
  },
  "browser": {
    "child_process": false,
    "fs": false,
    "module": false
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.5.0",
    "@docusaurus/mdx-loader": "^3.5.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@docusaurus/plugin-content-docs": "^3.6.3",
    "@docusaurus/types": "^3.6.3",
    "@docusaurus/utils": "^3.6.3",
    "@vscode/codicons": "^0.0.36",
    "marked": "^9.1.6",
    "marked-smartypants": "^1.1.9",
    "typedoc": "^0.27.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.6.3",
    "@docusaurus/tsconfig": "^3.6.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.2"
  }
}